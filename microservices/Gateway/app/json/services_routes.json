{
    "controller": {
        "HOST": "http://127.0.0.1",
        "PORT": "5000",
        "controller": {
            "route_parameter": "/controller",
            "endpoints": {
                "home": "/"
            }
        }
    },
    "log": {
        "HOST": "http://127.0.0.1",
        "PORT": "5001",
        "report_log": {
            "route_parameter": "/log",
            "endpoints": {
                "debug": "/debug",
                "info": "/info",
                "warning": "/warning",
                "error": "/error",
                "critical": "/critical"
            }
        }
    }
}